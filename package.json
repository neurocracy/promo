    "autoprefixer": "^10.4.4",
    "copy-webpack-plugin": "^11.0.0",
    "corepack": "^0.30.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.11.0",
    "favicons": "^7.1.4",
    "favicons-webpack-plugin": "^6.0.0",
    "file-loader": "^6.2.0",
    "glob": "^11.0.0",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.4.49",
    "postcss-loader": "^7.3.3",
    "sass": "^1.43.4 <1.79.0",
